#: pacaur:959
msgid "usage:  pacaur <operation> [options] [package(s)]"
msgstr "składnia:  pacaur <operacja> [opcje] [pakiet(y)]"

#: pacaur:960
msgid "operations:"
msgstr "operacje:"

#: pacaur:961
msgid " pacman extension"
msgstr "rozszerzenie pacmana"

#: pacaur:962
msgid "   -S, -Q           extend pacman operations to the AUR"
msgstr "   -S, -Q           rozciąga działanie operacji pacmana na AUR"

#: pacaur:963
msgid " AUR only"
msgstr "Tylko AUR"

#: pacaur:964
msgid "   -s, --search     search AUR repository for matching strings"
msgstr "   -s, --search     poszukuje pasujących ciągów tekstu w AUR"

#: pacaur:965
msgid "   -i, --info       view package information -- pass twice for details"
msgstr ""
"   -i, --info       pokazuje informacje o pakiecie -- wprowadź podwójnie, by "
"zobaczyć szczegóły"

#: pacaur:966
msgid ""
"   -d, --download   download target(s) -- pass twice to download AUR "
"dependencies"
msgstr ""
"   -d, --download   pobiera pakiet(y)  -- wprowadź podwójnie, by pobrać "
"zależności z AUR"

#: pacaur:967
msgid "   -m, --makepkg    download and make target(s)"
msgstr "   -m, --makepkg    pobiera i buduje pakiet(y)"

#: pacaur:968
msgid "   -y, --sync       download, make and install target(s)"
msgstr "   -y, --sync       pobiera, buduje i instaluje pakiet(y)"

#: pacaur:969
msgid "   -k, --check      check for AUR update(s)"
msgstr "   -k, --check      sprawdza aktualność pakietu/ów z AUR"

#: pacaur:970
msgid "   -u, --update     update AUR package(s)"
msgstr "   -u, --update     aktualizuje pakiet(y) z AUR"

#: pacaur:971 pacaur:980
msgid " general"
msgstr "ogólne"

#: pacaur:972
msgid "   -v, --version    display version information"
msgstr "   -v, --version    wyświetla informacje o wersji"

#: pacaur:973
msgid "   -h, --help       display help information"
msgstr "   -h, --help       wyświetla niniejszą pomoc"

#: pacaur:974
msgid "   --fixbackend     quickly rebuild backend"
msgstr "   --fixbackend     szybka przebudowa backendu"

#: pacaur:976
msgid "options:"
msgstr "opcje:"

#: pacaur:977
msgid ""
" pacman extension - can be used with the -S, -Ss, -Si, -Sii, -Sw, -Su, -Qu, -"
"Sc, -Scc operations"
msgstr ""
" rozszerzenie pacmana - można użyć z -S, -Ss, -Si, -Sii, -Sw, -Su, -Qu, -Sc, "
"-Scc"

#: pacaur:978
msgid "   -a, --aur        only search, install or clean packages from the AUR"
msgstr "   -a, --aur        szuka, instaluje lub czyści tylko pakiety z AUR"

#: pacaur:979
msgid ""
"   -r, --repo       only search, install or clean packages from the "
"repositories"
msgstr ""
"   -r, --repo       szuka, instaluje lub czyści tylko pakiety z repozytoriów"

#: pacaur:981
msgid "   -e, --edit       edit target(s) PKGBUILD and view install script"
msgstr ""
"   -e, --edit       edycja plików PKGBUILD oraz podgląd skryptów .install"

#: pacaur:982
msgid ""
"   -c, --clean      clean target(s) build files -- can be combined with the -"
"m, -y, -u operations"
msgstr ""
"   -c, --clean      czyści pliki po budowaniu pakietu -- można łączyć z -m, -"
"y, -u"

#: pacaur:983
msgid "   -q, --quiet      show less information for query and search"
msgstr ""
"   -q, --quiet      wyświetla mniej informacji przy zapytaniach i "
"wyszukiwaniu"

#: pacaur:984
msgid "   --devel          consider AUR development packages upgrade"
msgstr "   --devel          uwzględnia aktualizacje pakietów devel z AUR"

#: pacaur:985
msgid ""
"   --ignore         ignore a package upgrade (can be used more than once)"
msgstr ""
"   --ignore         pomija aktualizacje pakietu/ów (można użyć wielokrotnie)"

#: pacaur:986
msgid "   --noconfirm      do not prompt for any confirmation"
msgstr "   --noconfirm      nie wyświetla żadnych potwierdzeń"

#: pacaur:987
msgid "   --noedit         do not prompt to edit files"
msgstr "   --noedit         wyłącza zapytanie o edycję plików"

#: pacaur:988
msgid "   --rebuild        always rebuild package(s)"
msgstr "   --rebuild        zawsze przebudowuje pakiet(y)"

